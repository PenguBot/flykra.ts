{
	"name": "@pengubot/flykra",
	"description": "Pengu's Flykra API Client",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "Aditya Nath Tripathi (adityatripathidelhi@gmail.com)",
	"license": "Apache-2.0",
	"private": false,
	"homepage": "https://github.com/pengubot/flykra.ts",
	"scripts": {
		"start": "node dist/index.js",
		"lint": "eslint src --ext ts --fix",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"sversion": "yarn standard-version",
		"_postinstall": "husky install",
		"prepublishOnly": "pinst --disable",
		"postpublish": "pinst --enable"
	},
	"dependencies": {
		"node-fetch": "^2.6.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@quantumly/eslint-config": "^1.1.0",
		"@types/module-alias": "^2.0.0",
		"@types/node": "^14.14.25",
		"@types/node-fetch": "^2.5.10",
		"@typescript-eslint/eslint-plugin": "^4.15.0",
		"@typescript-eslint/parser": "^4.15.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.19.0",
		"husky": "^5.0.9",
		"lint-staged": "^10.5.4",
		"pinst": "^2.1.4",
		"standard-version": "^9.1.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.4"
	},
	"files": [
		"src"
	],
	"engines": {
		"node": ">=15",
		"npm": ">=6"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix --ext ts"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	}
}
